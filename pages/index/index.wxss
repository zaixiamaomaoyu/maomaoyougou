/* 轮播图 */
.index_swiper swiper{
    height: 340rpx;
} 
.index_swiper image{
    width: 100%;
}


/* 导航分类 */
.index_cate{
    display: flex;
} 
.index_cate navigator{
    flex: 1;
    padding: 20rpx;
}
.index_cate navigator image{
    width: 100%;
}


/* 楼层 */
.floor_title{
    padding: 10rpx 0;
}
.floor_title image{
    width: 100%;
}

.floor_content navigator{
float: left;
width: 33.33vw;
}
/* .floor_content navigator:nth-child(-n+4){
    height: 27.333333vw;

} */

.floor_content navigator:nth-last-child(-n+4) {
    /* 原图的宽高 232 *386 */
    height: 27.72711207vw;
    border-left: 10rpx solid #fff;
  }
 .floor_content navigator:nth-child(2),
 .floor_content navigator:nth-child(3) {
    border-bottom: 10rpx solid #fff;
  }
 .floor_content navigator image {
    width: 100%;
    height: 100%;
  }
  